	<bean class="com.cg.demo.labC.Employee">
	<property name="employeeId" value="116"></property>
	<property name="employeeName" value="Anusha"></property>
	<property name="salary" value="50000"></property>
	</bean>
	</list>
	</property>
	</bean>

</beans>
